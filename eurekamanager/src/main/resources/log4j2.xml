<?xml version="1.0" encoding="UTF-8" ?>
<configuration status="WARN">
    <appenders>
        <Console name="Console" target="SYSTEM_OUT">
            <PatternLayout pattern="[%d{HH:mm:ss:SSS}] [%p] - %l - %m%n" />
        </Console>
    </appenders>
    <loggers>
        <root level="all">
            <appender-ref ref="Console" />
        </root>
    </loggers>
</configuration>